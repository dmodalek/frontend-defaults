{
  "name": "@namics/tslint-config",
  "description": "Shared tslint config",
  "version": "0.2.1",
  "repository": "https://github.com/namics/frontend-defaults",
  "main": "./build/tslint.json",
  "license": "MIT",
  "author": "Namics AG",
  "contributors": [
    "Simon Mollweide <simon.mollweide@namics.com>"
  ],
  "private": false,
  "engines": {
    "node": ">=8"
  },
  "files": [
    "README.md",
    "LICENSE",
    "build"
  ],
  "scripts": {
    "build": "node bin/build-tslint-config.js",
    "prepublishOnly": "npm-run-all test build",
    "pretest": "npm run build",
    "test": "jest",
    "watch-test": "jest --watchAll",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint '__test__/**/*.{ts,tsx}'"
  },
  "peerDependencies": {
    "tslint": "5.x"
  },
  "devDependencies": {
    "jest": "24.7.1",
    "rimraf": "2.6.3",
    "tslint": "5.15.0",
    "typescript": "3.4.3",
    "npm-run-all": "4.1.5"
  },
  "dependencies": {
    "tslint-react": "4.0.0"
  }
}